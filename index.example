<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/tailwindcss/2.2.4/tailwind.min.css' />
    <script src="https://kit.fontawesome.com/f3057fdcec.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="styles/007.css">
  <link rel="shortcut icon" href="https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/apple/325/upside-down-face_1f643.png" type="image/x-icon">
    <title>thuk xatspace</title>
  </head>
  <body class="padding: 0rem;">
    <div class="xp_profile">
        <span class="xp_profile_bg"></span>
        <span id="userAvatar"></span><i class="fab fa-spotify" style="color: #ffffff;"></i>
        <div class="xp_profile_name">
          <p id="username"></p>
          <p id="status"></p>
          <div class="xp_gradient_bar"></div>
        </div>
    </div>
    <div class="list2">
        <a>
            <span id="listeningto"></span>
        </a>
    </div>
    <div class="list">
        <a class="fixed mt-8 top-0 right-0 items-center h-8 px-4-status m-2 text-sm text-indigo-100 transition-colors duration-150 bg-gray-800 rounded-lg focus:shadow-outline hover:bg-gray-800" id="onlineStatus"></a>
         <!-- 666 -->
         <a>
            <div class='card' onclick='window.open("https://xat.com/666")'>
                <div style='display: flex;'>
                    <img src='https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/microsoft/310/alien_1f47d.png' class='card-img' draggable=false>
                    <span class='card-title'>666 chat</span>
                </div>
                <p class='card-desc'>
                    My personal xat chat.
                </p>
                <p class='card-url'>
                    👻 💀 ☠️ 👽
                </p>
            </div>
        </a>
        <!-- Ajuda -->
        <a>
            <div class='card' onclick='window.open("https://xat.com/Ajuda")'>
                <div style='display: flex;'>
                    <img src='https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/samsung/320/question-mark_2753.png' class='card-img' draggable=false>
                    <span class='card-title'>Ajuda chat (Portuguese help chat)</span>
                </div>
                <p class='card-desc'>
                    Brazil/Portugal help chat.
                </p>
                <p class='card-url'>
                    😋 ❓
                </p>
            </div>
        </a>
        <!-- xatRadio -->
        <a>
            <div class='card' onclick='window.open("https://xatRadio.com")'>
                <div style='display: flex;'>
                    <img src='https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/samsung/320/musical-note_1f3b5.png' class='card-img' draggable=false>
                    <span class='card-title'>xatRadio players</span>
                </div>
                <p class='card-desc'>
                    Upload your players to your chat!
                </p>
                <p class='card-url'>
                    📻 🎶
                </p>
            </div>
        </a>
        <!-- Forum -->
        <a>
            <div class='card' onclick='window.open("https://forum.xat.com/profile/41653/")'>
                <div style='display: flex;'>
                    <img src='https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/apple/325/anger-symbol_1f4a2.png' class='card-img' draggable=false>
                    <span class='card-title'>Forum profile</span>
                </div>
                <p class='card-desc'>
                    My forum page...
                </p>
                <p class='card-url'>
                    📚 ⚡
                </p>
            </div>
        </a>
        <!-- xatProject -->
        <a>
            <div class='card' onclick='window.open("https://thuk.xatproject.com")'>
                <div style='display: flex;'>
                    <img src='https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/apple/325/anger-symbol_1f4a2.png' class='card-img' draggable=false>
                    <span class='card-title'>Personal web page</span>
                </div>
                <p class='card-desc'>
                    My web page...
                </p>
                <p class='card-url'>
                    👽 👻
                </p>
            </div>
        </a>
        <!-- Github -->
        <a>
            <div class='card' onclick='window.open("https://github.com/xathuk?tab=repositories")'>
                <div style='display: flex;'>
                    <img src='https://github.githubassets.com/images/modules/logos_page/GitHub-Mark.png' class='card-img' draggable=false>
                    <span class='card-title'>Github</span>
                </div>
                <p class='card-desc'>
                    Feel free to use my xatframes!
                </p>
                <p class='card-url'>
                    👽 👻
                </p>
            </div>
        </a>
        <br>
    </div>
    <script src="xss/xss.js"></script>
</body>
</html>
